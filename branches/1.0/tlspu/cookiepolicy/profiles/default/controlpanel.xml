<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="tlspu.cookiepolicy">
  <configlet title="Cookie Policy"
             action_id="cookiepolicy"
             appId="tlspu.cookiepolicy"
             category="Products"
             condition_expr=""
             url_expr="string:${portal_url}/@@cookiepolicy"
             icon_expr="string:${portal_url}/++resource++tcp_images/ico-cookiepolicy.png"
             visible="True"
             i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>
</object>
